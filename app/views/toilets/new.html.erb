<div class="container">
  <h1>Add a new toilet</h1><br>

  <%= form_for(@toilet) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :price %>
    <%= f.number_field :price %>
    <%= f.submit %>
  <% end %>
</div>
